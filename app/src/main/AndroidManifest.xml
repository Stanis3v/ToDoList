<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="stanis3v.lab.todolist">

    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_v5"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_v5"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".About.AboutActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Setting.SettingActivity" android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Create.CreateActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>